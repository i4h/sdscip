{UTF-8}
Regeneration=
	REGENERATIONRATE*(0*IF THEN ELSE(DAMAGETHRESHOLD>Pollution, Pollution, DAMAGETHRESHOLD\
		) + MIN(Pollution, DAMAGETHRESHOLD))
	~	1/Year
	~		|

ConsumptionLevel=
	Assets
	~	1
	~		|

AssetGrowth=
	GROWTHRATE*ConsumptionLevel*Pollution*(1-(ConsumptionLevel*Pollution/CONSUMPTIONTARGET\
		))
	~	1/Year
	~		|

Births=
	BIRTHRATE*Population*EnvironmentalHealth*ConsumptionLevel*BIRTHCONTROL
	~	1/Year
	~		|

Destruction=
	POLLUTIONRATE*Population*ConsumptionLevel
	~	1/Year
	~		|

INITIALPOPULATION=
	1
	~	1
	~		|

INITIALASSETS=
	1
	~	1
	~		|

INITIALPOLLUTION=
	1
	~	1
	~		|

Population= INTEG (
	Births-Deaths,
		INITIALPOPULATION)
	~	1 [0,?]
	~		|

Pollution= INTEG (
	+Destruction-Regeneration,
		INITIALPOLLUTION)
	~	1 [0,?]
	~		|

Assets= INTEG (
	AssetGrowth,
		INITIALASSETS)
	~	1 [0,?]
	~		|

REGENERATIONRATE=
	0.1
	~	1/Year
	~		|

BIRTHCONTROL=
	1
	~	1
	~		|

BIRTHRATE=
	0.03
	~	1/Year
	~		|

GROWTHRATE=
	0.05
	~	1/Year
	~		|

CONSUMPTIONTARGET=
	10
	~	1
	~		|

DAMAGETHRESHOLD=
	1
	~	1
	~		|

Deaths=
	DEATH RATE*Population*Pollution
	~	1/Year
	~		|

DEATH RATE=
	0.01
	~	1/Year
	~		|

EnvironmentalHealth=
	DAMAGETHRESHOLD/Pollution
	~	1
	~		|

POLLUTIONRATE=
	0.02
	~	1/Year
	~		|

********************************************************
	.Control
********************************************************~
		Simulation Control Parameters
	|

FINAL TIME  = 200
	~	Year
	~	The final time for the simulation.
	|

INITIAL TIME  = 0
	~	Year
	~	The initial time for the simulation.
	|

SAVEPER  = 
        TIME STEP
	~	Year [0,?]
	~	The frequency with which output is stored.
	|

TIME STEP  = 0.5
	~	Year [0,?]
	~	The time step for the simulation.
	|

